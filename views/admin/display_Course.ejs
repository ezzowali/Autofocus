


<%- include('.././includes/includes_nav/navigation_admin') %>


<%- include('../includes/links') %>


<link rel="stylesheet" href="css/item_User.css">


  <!-- just to make scrolling effect possible -->
  <section id="team" class="pb-5">
    <div class="container">
        <h5 class="section-title ">الدورات </h5>
        <div class="row">

            
            <%for (var i = 0; i < data.length ; i++) {%>

               
                <%for (var j=1; j< data[i].courses.length; j++) {%>

                    
            <!-- Team member -->
     
                <div class="col-xs-12 col-sm-6 col-md-4">
                    <div class="image-flip">
                    <div class="mainflip flip-0">
  
                      
                        <div class="frontside">
                            <div class="card " >
                                <div class="card-body text-center">
                                 
                          
                                       
                                   
  
                                    

                                    <h4>
                                        :عنوان الدورة 
                                      </h4>
                                   

                                 
                                    <p class="card-text">  
                                        <%=data[i].courses[j].CourseName%></p><hr>

                                   

                                        <h4>
                                            :محاور الدورة 
                                          </h4>

                                    <h4 class="card-title"><%=data[i].courses[j].titlesCourse%></h4><hr>
                                    

                                 


                                
                                    <h4>فترة الدورة</h4>
                                    <h4 class="card-title">

                                        من
                                        <%=data[i].courses[j].begining%> 

                                       
          
                                        <br>
                                       
                                         <h4 class="card-title"> الى</h4>
                 <h4 class="card-title"><%=data[i].courses[j].ending%></h4> 
                                    </h4> <hr>





                                    <h4 style="font-family: 'Cairo', sans-serif; color: orange;">
                                        الدورة ستكون 
                                        <%=data[i].courses[j].place%>
                                        
                                    </h4>


                                    <%if(data[i].courses[j].city!==null){%>

                                        <hr>
                                        <h4 class="card-title">
                                            <%=data[i].courses[j].city%>
                                        </h4>

                                    <%}%>

                                    <%=data[i].courses[j].accept%>

                                </div>
                            </div>
                        </div>

                        <div class="backside">
                            <div class="card">
                                <div class="card-body text-center mt-4">

                                    <h4> :شروط الدورة  </h4> 
                                      
                                    <h4 class="card-title">
                                        
                                        <%=data[i].courses[j].courseCondeitions%>
                                    </h4><hr>
                         
                                    

                                    <h4> : اسم مقدم الدورة    </h4> 
                                    <h4  style="font-family: 'Cairo', sans-serif; color: orange;" >
                                        <%= data[i].firstNameA%>    <%= data[i].lastNameA%>
  
                                    </h4>
                                    <hr>

                                      
                       
                                    <p class="card-title">  للتواصل  0<%= data[i].phone%></p>                                
  




                                    <div class="col-md-2">




                                        <form action="/accept_Course" method="POST">
                                            <input type="hidden" name="email" value="<%=data[i].email%>">

                             


                                            <input type="hidden" name="number" value="<%=j%>">

                                            <input type="hidden" name="_csrf" value="<%= csrfToken %>">




                                            <button type="submit" class="btn"><i class="fa-2x fas fa-check"
                                                    style="color:#4be702;"></i></button>

                                        </form>


                                    </div>

                                    <div class="col-md-2">

                                        <form action="/wait_Course" method="POST">
                                            <input type="hidden" name="email" value="<%=data[i].email%>">
                                            <input type="hidden" name="number2" value="<%=j%>">
                                        


                                            <input type="hidden" name="_csrf" value="<%= csrfToken %>">



                                            <button type="submit" class="btn"><i class="fa-2x fas fa-clock"
                                                    style="color:#beecdf;"></i></button>

                                        </form>
                                    </div>


                                    <div class="col-md-2">

                                        <form action="/refuse_Course" method="POST">
                                            <input type="hidden" name="email" value="<%=data[i].email%>">
                                            <input type="hidden" name="number3" value="<%=j%>">
                                        


                                            <input type="hidden" name="_csrf" value="<%= csrfToken %>">



                                            <button type="submit" class="btn"><i class="fa-2x fas fa-times"
                                                    style="color:#fa0000;"></i></button>

                                        </form>
                                    </div>

                            

                                </div>
                            </div>
                        </div>
  
                        
                        
  
                    </div>
                </div>
            </div>
   


       
        <%}%>

                <%}%>


            </div>
        </div>
        
        </section>
        
        
        
        <%- include('../includes/footer') %>