<body style="background-color:	#6b65ad; ">
  
  <div class="container mt-5">
  
    <% if (message) { %>
      <div class="alert alert-danger alert-dismissible fade show">
        <strong>!!خطأ</strong> هذا الايميل موجود بالفعل 


            <button type="button" class="close" data-dismiss="alert">&times;</button>
      

    </div>
  <% } %>


  <% if (message2) { %>

  
    <div class="alert alert-success" role="alert">
    مبروك لقد تم انشاء الحساب بنجاح <a href="/" class="alert-link">اضغط هنا للذهاب الى صفحة الدخول </a>
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
   
  
<% } %>
    <div class="row">
      <div class="col-sm-8">
        <div class="card content" style="background-color:	#6b65ad;" >
         
          <div class="card-body content" style="color: white;  text-align:left; ">
            <h1> التسجيل للمصورين</h1>
                 
            <form action="/Equipment_sellerRegister" method="POST">
  
              <div class="col-md-6 mb-3">
  
  
            <label for="firstName">الاسم الاول </label>
            <input type="text" class="form-control y" id="firstNameA" name="firstNameA" value="" required>
            <div class="invalid-feedback">
              Valid first name is required.
            </div>
          </div>


          <div class="col-md-6 mb-3">
  
  
            <label for="lastNameA">الاسم الاخير </label>
            <input type="text" class="form-control y" id="lastNameA" name="lastNameA" value="" required>
            <div class="invalid-feedback">
              Valid first name is required.
            </div>
          </div>
  
          <div class="col-md-6 mb-3">
            <label for="email">الايميل</label>
          <input type="email" class="form-control" name="email" value="" required>
        </div>
  
  
      
  
       
  
  
            <div class="col-md-6 mb-3">
             <label for="password">كلمة المرور</label>
             <input type="password" class="form-control" name="password" value="">
           </div>

<!-- //////// -->





  <div class="col-md-6 mb-3">
    <label for="phone">رقم الجوال</label>
  <input type="number" class="form-control" name="phone" value="" required>
</div>
  
  
  <!-- /////////// -->
           <div class="col-md-6 mb-3">
    <input type="hidden" name="_csrf" value="<%= csrfToken %>">
    <button type="submit" class="btn btn-info">تسجيل</button>
  </div>
            </form>
          </div>
        </div>
      </div>
  
  
  
  
  
    </div>
  </div>


  <script>

 
var sel1 = document.querySelector('#sel1');
  var sel2 = document.querySelector('#sel2');
  var options2 = sel2.querySelectorAll('option');
  
  function giveSelection(selValue) {
    sel2.innerHTML = '';
    for(var i = 0; i < options2.length; i++) {
      if(options2[i].dataset.option === selValue) {
        sel2.appendChild(options2[i]);
      }
    }
  }
  
  giveSelection(sel1.value);






    var closebtns = document.getElementsByClassName("close");
var i;


  closebtns.addEventListener("click", function() {
    this.parentElement.style.display = 'none';
  });


  </script>

</body>




