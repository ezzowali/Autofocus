  <script>
    $(document).ready(function() {
    var max_fields = 10;
    var wrapper = $(".container1");
    var add_button = $(".add_form_field");

    var x = 1;
    $(add_button).click(function(e) {
        e.preventDefault();
        if (x < max_fields) {
            x++;
            $(wrapper).append('<div><textarea class="form-control" minlength="10"  style="margin-top: 1rem;" type="text" name="exper"></textarea><button style="margin-top: 1rem;" class="delete btn btn-danger"> Delete</button></div>'); //add input box
        } else {
            alert('You Reached the limits')
        }
    });

    $(wrapper).on("click", ".delete", function(e) {
        e.preventDefault();
        $(this).parent('div').remove();
        x--;
    })
});


var closebtns = document.getElementsByClassName("close");
var i;


  closebtns.addEventListener("click", function() {
    this.parentElement.style.display = 'none';
  });


    </script>



